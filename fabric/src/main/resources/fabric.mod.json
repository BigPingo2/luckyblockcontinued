{
  "schemaVersion": 1,
  "id": "lucky",
  "version": "${modVersion}",

  "name": "Lucky Block",
  "description": "Adds a block with random outcomes",
  "authors": [
    "Alex Socha"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/lucky-block",
    "sources": "https://github.com/alexsocha/luckyblock"
  },

  "license": "All rights reserved",
  "icon": "assets/lucky/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "mod.lucky.fabric.FabricMod"
    ],
    "fabric-datagen": [
      "mod.lucky.fabric.FabricModDataGen"
    ],
    "client": [
      "mod.lucky.fabric.FabricModClient"
    ]
  },

  "depends": {
    "fabricloader": ">=${minFabricLoaderVersion}",
    "fabric": "*",
    "minecraft": ">=${minMinecraftVersion}"
